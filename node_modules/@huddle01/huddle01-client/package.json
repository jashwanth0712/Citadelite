{
  "name": "@huddle01/huddle01-client",
  "version": "0.0.10",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "yarn build --watch",
    "clean": "rm -rf dist",
    "build": "yarn clean && tsup src --format esm --dts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    },
    "./hooks": {
      "import": "./dist/hooks/index.mjs"
    },
    "./schema": {
      "import": "./dist/schema/index.mjs"
    },
    "./store": {
      "import": "./dist/store/index.mjs"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "hooks": [
        "dist/hooks/index.d.ts"
      ],
      "schema": [
        "dist/schema/index.d.ts"
      ],
      "store": [
        "dist/store/index.d.ts"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/protoo-client": "^4.0.1",
    "@types/random-string": "^0.0.28",
    "@types/react-dom": "^18.0.1",
    "autoprefixer": "^10.4.2",
    "config": "*",
    "eslint": "^8.18.0",
    "postcss": "^8.4.5",
    "tsconfig": "*",
    "tsup": "^6.3.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "hark": "^1.2.3",
    "mediasoup-client": "^3.6.51",
    "protoo-client": "^4.0.6",
    "random-string": "^0.2.0",
    "react": "^18.2.0",
    "zustand": "^4.0.0-rc.1"
  },
  "keywords": [
    "huddle01-client",
    "huddle01",
    "sdk",
    "video-conferencing",
    "web3",
    "blockchain"
  ]
}
